// Live earnings simulation
const generateEarning = () => {
  const brands = ['Nike', 'Adidas', 'Under Armour', 'Peloton', 'Apple Fitness'];
  const campaigns = ['Summer Fitness', 'Morning Warriors', 'Premium Athletes'];
  
  return {
